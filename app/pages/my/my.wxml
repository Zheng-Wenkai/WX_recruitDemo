<!--个人页面-->
<view class='container'>
    <!-- <image class='dashed' src="/images/index/dashed.png"></image> -->
    <image class='background' src='/images/my/background.jpg'></image>
    <image class='avatar' src='/image/my/avatar.jpg'></image>
    <view class='content1'>
        <text class='ID'>编号：{{14}}</text>
        <view class='btnContent'>
            <button class='btn1'>报名</button>
            <button class='btn2' bindtap='searchRangeState'>查询排队情况</button>
        </view>
    </view>
    <view class='border'></view>

    <view class='content2'>
        <view class='data'>姓名：</view>
        <view class='data'>学院：</view>
        <view class='data'>专业：</view>
        <view class='data'>面试时间：</view>
        <view class='data'>部门意愿：</view>
        <view class='data'>报名类型：</view>
    </view>

    <view class='border'></view>

    <button class='btn3' bindtap='searchResult'>查询面试结果</button>

</view>